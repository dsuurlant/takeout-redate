{
    "alias": "takeout-redate.phar",
    "banner": [
        "Takeout Redate CLI Tool",
        "",
        "@release-version@"
    ],
    "compression": "GZ",
    "directories": [
        "src"
    ],
    "files": [
        "bin/app",
        "vendor/autoload.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Tests",
                "tests",
                "phpstan",
                "phpunit",
                "php-cs-fixer",
                "humbug"
            ],
            "in": "vendor"
        },
        {
            "in": "vendor/symfony/console/Resources"
        },
        {
            "in": "vendor/composer"
        }
    ],
    "git-version": "git-version",
    "output": "takeout-redate.phar",
    "php-scoper": null,
    "metadata": {
        "version": "@git-version@",
        "built": "@datetime@"
    }
}

