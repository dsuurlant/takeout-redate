{
    "name": "takeout-redate/cli",
    "description": "A Symfony Console CLI tool",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "symfony/console": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "humbug/box": "^4.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "TakeoutRedate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TakeoutRedate\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/app"
    ],
    "scripts": {
        "test": "phpunit",
        "build": "box compile",
        "cs-fix": "php-cs-fixer fix",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "phpstan": "phpstan analyse",
        "quality": [
            "@cs-fix",
            "@phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
